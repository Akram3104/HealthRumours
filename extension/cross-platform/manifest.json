{
    "manifest_version":3,
    "name": "Testing",
    "version": "1.0",
    "author": "@IIITT",
    "permissions":[
        "activeTab",
        "scripting",
        "tabs"
    ],
    "background":{
        "service_worker":[
            "background.js",
            "popup.js"
        ]
    },
	"permissions":[
        "tabs",
        "http://*/",
        "https://*/"
    ],
    "action":{
        "default_icon": "logo.png",
		"default_popup": "popup.html"
    }
}