{
    "manifest_version":2,
    "name":"firefox",
    "version":"1.0",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "contextMenus"
    ],
    "browser_action":{
       "default_icon":"fake.png"
    },
    "content_scripts": [
        {
          "matches": ["https://web.telegram.org/*"],
          "js": ["telegram.js"]
        },
        {
          "matches": ["https://web.whatsapp.com/*" ],
          "js": ["whatsapp.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
}